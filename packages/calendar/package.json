{
  "name": "@mui-toolpad-extended-tuni/calendar",
  "version": "3.0.0",
  "description": "Calendar microservice for MUI Toolpad Extended TUNI",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"

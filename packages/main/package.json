{
  "name": "@mui-toolpad-extended-tuni/main",
  "version": "3.3.0",
  "description": "Main package for MUI Toolpad Extended TUNI - provides ToolpadProvider, LMS components, and platform features",
  "author": "Jaakko Rajala",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jalez/mui-toolpad-extended-tuni.git",
    "directory": "packages/main"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.es.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "mui",
    "toolpad",
    "education",
    "tuni",
    "main"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@toolpad/core": "^0.16.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@fontsource/roboto": "^5.0.14",
    "axios": "^1.7.9",
    "chart.js": "^4.5.1",
    "d3-force": "^3.0.0",
    "lodash": "^4.17.21",
    "react-chartjs-2": "^5.2.0",
    "react-colorful": "^5.6.1",
    "react-grid-layout": "^1.5.0",
    "styled-components": "^6.1.0",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "@mui-toolpad-extended-tuni/core": "^3.1.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/x-date-pickers": "^7.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "zustand": "^4.5.0"
  }
}
